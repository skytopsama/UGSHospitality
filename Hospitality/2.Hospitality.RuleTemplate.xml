      <name>UGS.Hospitality.UGS1</name>
      <description>UGS Review and Approval</description>
      <attributes><!-- Forms go to Kim regardless of Campus, remove attributes? -->
        <attribute>
          <name>UGSCampusAttribute</name>
          <required>true</required>
        </attribute>
      </attributes>
    </ruleTemplate>

    <!-- Initiator FYI -->
    <ruleTemplate allowOverwrite="true">
      <name>UGS.Hospitality.InitiatorFYI</name>
